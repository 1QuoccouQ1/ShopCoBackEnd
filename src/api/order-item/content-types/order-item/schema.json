{
  "kind": "collectionType",
  "collectionName": "order_items",
  "info": {
    "singularName": "order-item",
    "pluralName": "order-items",
    "displayName": "Order-item"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "product_name": {
      "type": "string"
    },
    "product_image": {
      "type": "text"
    },
    "price": {
      "type": "decimal"
    },
    "quantity": {
      "type": "integer"
    },
    "color": {
      "type": "string"
    },
    "size": {
      "type": "string"
    },
    "product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.product"
    },
    "order": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::order.order",
      "inversedBy": "order_items"
    }
  }
}
